		<nav class="grey darken-4">
   <div class="nav-wrapper">
	 <a href="#" class="brand-logo">Intraspect</a>
	 <a href="#" data-activates="mobile-demo" class="button-collapse"></a>

   </div>
 </nav>

	{{#if message}}
        		<div class="alert alert-danger">{{message}}</div>
    	{{/if}}
	<div class="container">

		<p class="flow-text center intro-text">Hello!</p>
		<p class="flow-text center welcome-text">Welcome to Intraspect</p>


		 <div class="row">


		 <form action="/login" method="post" class="form--login">
			 <center>
			 	<span class="center form__label">Returning back? Log in!</span>
			 	<hr>
			 <center>
        			<div class="form-group">
							<center>
								<label>Email</label>
            					<input type="email" class="form-control" name="email" class="validate" placeholder="email">
							</center>
        			</div>
        			<div class="form-group">
							<center>
								<label>Password</label>
            					<input type="password" class="form-control" name="password" placeholder="password">
							</center>
        			</div>

        			<button type="submit" class="btn btn-warning btn-lg grey darken-4 btn-large"><span class="button-text">Login</span></button>
        			<!-- <a href="/signup" class="btn btn-warning btn-lg grey darken-4 btn-large"><span class="button-text">Signup</span></a> -->
    		</form>

    <!-- <a href="/auth/facebook" class="btn btn-primary btn-large grey darken-4"><span class="fa fa-facebook"></span> <span class="button-text">Facebook</span></a> -->
		</div>

		<hr>

        <p class="flow-text center">Don't have an account?  <a href="/signup">Signup</a>.</p>

</div>

